<div class="container">
	<div class="row">
	
		<%=simple_form_for @product do |f|%>
		<fieldset>
		<%=f.input :name %>
		<%=f.input :price%>
		<%=f.input :description%>
		<%=f.input :manufacture_year,class: 'form-control datepicker'%>
		<%=f.button :submit %>
		</fieldset>
		<%end%>
		
	</div>
</div>